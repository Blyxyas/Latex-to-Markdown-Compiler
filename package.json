{
	"name": "latexcompiler",
	"displayName": "Latex to Markdown Compiler",
	"description": "Compile your latex-containing markdown to markdown that can be view from a Github Markdown File",
	"version": "1.1.3",
	"repository": "https://github.com/Blyxyas/Latex-to-Markdown-Compiler/",
	"publisher": "Blyxyas",
	"icon": "images/icon.png",
	"license": "MIT",
	"engines": {
	"vscode": "^1.65.0"
	},
	"categories": [
		"Formatters",
		"Other",
		"Notebooks"
	],
	"activationEvents": [
		"onCommand:latexcompiler.compile"
	],
  "main": "./out/extension.js",
  "contributes": {
	"configuration": [
		{
			"id": "latexcompiler",
			"title": "Latex to Markdown Compiler",
			"properties": {
				"latexcompiler.backgroundColor": {
					"type":"string",
					"order": 1,
					"default": "#333",
					"description": "Background Color"
				},
				"latexcompiler.textColor": {
					"type":"string",
					"order": 2,
					"default": "#fff",
					"description": "Text Color"
				}
			}
		}
	],
	"commands": [
	  {
		"command": "latexcompiler.compile",
		"title": "<=== Compile Latex to Markdown ===>"
	  }
	]
  },
  "scripts": {
	"vscode:prepublish": "npm run compile",
	"compile": "tsc -p ./",
	"watch": "tsc -watch -p ./",
	"pretest": "npm run compile && npm run lint",
	"lint": "eslint src --ext ts",
	"test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
	"@types/glob": "^7.2.0",
	"@types/mocha": "^9.1.0",
	"@types/node": "14.x",
	"@types/vscode": "^1.65.0",
	"@typescript-eslint/eslint-plugin": "^5.16.0",
	"@typescript-eslint/parser": "^5.16.0",
	"@vscode/test-electron": "^2.1.3",
	"eslint": "^8.11.0",
	"glob": "^7.2.0",
	"mocha": "^9.2.2",
	"typescript": "^4.5.5"
  },
  "dependencies": {
	"vsce": "^2.7.0"
  }
}
